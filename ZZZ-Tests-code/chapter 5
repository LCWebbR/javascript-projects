// pretty obvious 

console.log(Boolean("true"));
console.log(Boolean("TRUE"));
console.log(Boolean(0));
console.log(Boolean(1));
console.log(Boolean(''));
console.log(Boolean('LaunchCode'));
// maybe empty string are false and 0 is false both by default without parameters?
console.log(Boolean(3));
console.log(Boolean('0'));
console.log(Boolean(""));
console.log(Boolean(10));
// seems to be true
//it could be lack of data

console.log(5 == 5);
console.log(5 == 6);
//5 is not 6 this one is easy
console.log(true == "true");
// false becuase the they are not the same because of the fact that one is a string and the other is a boolean?


console.log("EQUALITY")

console.log(7 == "7");//a
console.log(0 == false);//b
console.log(0 == '');//c
console.log ("4" == (2 * 2)) //d
console.log (7 === (1 * 7))//e
//a true. its javascript convert data types with == to the same data type to compare if they are true
//b true because false = 0 in JS
//c because both are false or no data?
//d same as a
//e same as a

//AND STATMENTS &&
console.log(7 > 5 && 5 > 3);
console.log(7 > 5 && 2 > 3);
console.log(2 > 3 && 'dog' === 'cat');
//true
//false 2 not greater 
// false string not the same and 2 not greater
console.log(4 > 3 && 'cat' === 'cat');
// works if the same

//OR STATMENTS ||
console.log(7 > 5 || 5 > 3);
console.log(7 > 5 || 2 > 3);
console.log(2 > 3 || 'dog' === 'cat');
//true both are true
//true one is true
//false both false

//Logical not 

console.log(!true);
console.log(!false);

//reverses the bloolean value

console.log( !(5 > 7) );
console.log( !('dog' === 'cat') );

// true because false 
//also true because false

